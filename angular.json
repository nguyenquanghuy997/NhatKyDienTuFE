              }
            ],
            "styles": [
              "node_modules/nouislider/dist/nouislider.min.css",
              "src/styles.scss",
              "src/assets/css/summernote/monokai.css",
              "src/assets/css/summernote/codemirror.css",
              "src/assets/css/summernote/codemirror.min.css",
              "src/assets/vendors/datatables/dataTables.bootstrap.min.css",
