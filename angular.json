              "src/assets/vendors/datatables/jquery.dataTables.min.js",
              "src/assets/vendors/datatables/dataTables.bootstrap.min.js",
              "node_modules/summernote/dist/summernote-lite.min.js",
              "node_modules/summernote/dist/lang/summernote-vi-VN.js",
              "src/assets/js/summernote/codemirror.js",
              "src/assets/js/summernote/formatting.js",
              "node_modules/summernote/dist/summernote-bs4.min.js",
              "src/assets/js/notificationCheck.js"
