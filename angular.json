{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "1fa46996-5e2c-4d4b-b54a-f533b5c256b3"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ES.NhatKyDienTu.Web": {
